.pageBox {
  position: relative;
}
.header {
  width: 100%;
  height: 360rpx;
  background-color: #313131;
  border-radius: 0 0 160rpx 160rpx;
  padding-top: 40rpx;
  margin-bottom: 100rpx;
}
.tip {
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding-right: 30rpx;
}
.tip > text {
  font-size: 28rpx;
  color: white;
  flex-shrink: 0;
}
.view {
  width: 380rpx;
  height: 60rpx;
  background-color: rgba(255, 255, 255, 0.3);
  border-radius: 0 30rpx 30rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
}
.view .imgs {
  height: 60rpx;
  width: 100rpx;
}
.view .imgs image {
  width: 40rpx;
  height: 40rpx;
}
.view text {
  font-size: 22rpx;
  color: white;
}
.card {
  display: flex;
  width: 90%;
  height: 330rpx;
  background-image: url(data:image/png;base64,);
  background-position: center center;
  background-size: 120% 100%;
  border-radius: 20rpx;
  box-sizing: border-box;
  margin: 30rpx auto;
  padding: 70rpx 30rpx;
}
.card image {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}
.card .TRight {
  padding-top: 10rpx;
  padding-left: 20rpx;
}
.card .TRight .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.card .TRight .title .text {
  color: #7b5b21;
  font-size: 34rpx;
  font-weight: bold;
}
.card .TRight .title .logo {
  width: 100rpx;
  height: 36rpx;
  background-color: #a5a5a5;
  background-size: 100% 100%;
  background-position: center center;
  border-radius: 18rpx;
  font-size: 25rpx;
  line-height: 36rpx;
  text-align: center;
  color: white;
}
.card .TRight .title .vip {
  background-color: #ee7b2d;
}
.card .TRight .text {
  color: #7b5b21;
  font-size: 28rpx;
}
.dataPackage {
  display: flex;
  justify-content: center;
}
.dataPackage .item {
  margin: 10rpx;
  background-color: #f9f9f9;
  border-radius: 15rpx;
  border: 2rpx solid #ccc;
  width: 28%;
  height: 240rpx;
  position: relative;
  display: flex;
  flex-direction: column;
}
.dataPackage .item .text {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex-grow: 1;
}
.dataPackage .active {
  background-color: #fbf7ef;
  border-color: #deb264;
}
.dataPackage .row {
  margin-top: 20rpx;
}
.dataPackage .month {
  font-size: 30rpx;
  font-weight: bold;
}
.dataPackage .discount {
  font-size: 26rpx;
  color: #deb264;
}
.dataPackage .discount text {
  font-size: 50rpx;
  font-weight: bold;
}
.dataPackage .money {
  color: #a5a5a5;
  font-size: 28rpx;
  text-decoration: line-through;
}
.dataPackage .recommendLogo {
  position: absolute;
  color: white;
  text-align: center;
  font-weight: bold;
  font-size: 28rpx;
  padding: 10rpx 20rpx;
  top: -26rpx;
  left: -6rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.ads {
  display: flex;
  align-items: center;
  padding: 30rpx;
}
.ads .logo {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #deb264;
  padding: 5rpx 20rpx;
  font-size: 28rpx;
  margin-right: 10rpx;
}
.ads text {
  color: #7b5b21;
  font-size: 24rpx;
}
.openBtn {
  width: 90%;
  margin: 0 auto 30rpx;
  color: #a98c4f;
  font-weight: bold;
  font-size: 34rpx;
  border-radius: 15rpx;
  background-color: #fcf1d3;
  text-align: center;
  padding: 20rpx 0;
}
.popBox {
  position: fixed;
  top: 0;
  height: 100vh;
  width: 100vw;
  display: none;
  justify-content: flex-end;
  flex-direction: column;
  z-index: 10;
  background-color: rgba(0, 0, 0, 0.4);
}
.popBox .blank {
  flex-grow: 1;
  width: 100%;
}
.popBox .box {
  height: 560rpx;
  width: 100%;
  background-color: white;
}
.popBox .box .title {
  text-align: center;
  padding: 20rpx 0;
}
.popBox .popLogo {
  background-image: url(data:image/png;base64,);
}
.openPopBox {
  display: flex;
}
.privilege {
  padding: 30rpx;
  border: 20rpx solid #eee;
  border-left: 0;
  border-right: 0;
}
.privilege .topic {
  font-size: 46rpx;
  font-weight: bold;
}
.privilege .item {
  padding: 20rpx 10rpx;
  display: flex;
  align-items: center;
}
.privilege .item image {
  width: 100rpx;
  height: 100rpx;
}
.privilege .item .right {
  margin-left: 20rpx;
}
.privilege .item .title {
  font-size: 30rpx;
}
.privilege .item .text {
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #999;
}
.instructions {
  padding: 30rpx;
}
.instructions .topic {
  font-size: 30rpx;
  font-weight: bold;
  margin: 0 0 20rpx;
}
.instructions .item {
  font-size: 24rpx;
  color: #bbb;
  margin: 10rpx 0;
}

