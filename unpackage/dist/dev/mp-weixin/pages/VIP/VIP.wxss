.pageTitle {
  display: flex;
  width: 100%;
  height: 100rpx;
  background-color: #ee7b2d;
}
.pageTitle .item {
  width: 22%;
  flex-shrink: 0;
  text-align: center;
  line-height: 100rpx;
  font-size: 25rpx;
  color: white;
}
.pageTitle .active {
  font-size: 40rpx;
  font-weight: bold;
}
.VIPcard {
  width: 91%;
  margin: 30rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  height: 320rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background-image: url(https://woniumd.oss-cn-hangzhou.aliyuncs.com/web/jianglan/202208130943758.png);
  background-size: 100% 100%;
  background-position: center center;
}
.VIPcard .top {
  display: flex;
  flex-grow: 1;
  padding: 30rpx;
}
.VIPcard .top image {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}
.VIPcard .bottom {
  background-size: 100% 100%;
  background-position: center center;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-image: url(data:image/png;base64,);
}
.TRight {
  padding: 20rpx;
  padding-right: 0;
  flex-grow: 1;
}
.TRight .title {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.TRight .title .text {
  color: #999;
  font-size: 25rpx;
}
.TRight .title .btn {
  width: 108rpx;
  height: 36rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-position: center center;
  font-size: 25rpx;
  line-height: 36rpx;
  text-align: center;
  color: #8a5736;
}
.TRight .text {
  color: white;
  font-size: 28rpx;
}
.BItem {
  padding: 0 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.BItem image {
  width: 40rpx;
  height: 40rpx;
}
.BItem text {
  margin-top: 10rpx;
  font-size: 20rpx;
  color: #c1aa90;
}
.recommend {
  width: 100%;
  background-color: white;
  padding: 20rpx 30rpx 0;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.recommend .contentX {
  width: 100%;
  height: 340rpx;
  flex-wrap: nowrap;
  flex-direction: row;
}
.recommend .contentX .item {
  width: 43%;
  flex-shrink: 0;
  margin: 0 10rpx;
}
.recommend .contentY {
  width: 100%;
  height: 70vh;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.recommend .contentY .item {
  width: 47%;
  flex-shrink: 1;
  padding-bottom: 20rpx;
  height: 350rpx;
  margin: 10rpx;
}
.recommend .content {
  display: flex;
  padding: 20rpx 0 0;
}
.recommend .content .item {
  box-sizing: border-box;
  background-color: #eee;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.recommend .content .item image {
  border-radius: 10rpx;
  width: 100%;
  height: 230rpx;
}
.recommend .content .item .title {
  color: #333;
  margin-top: 10rpx;
  font-size: 26rpx;
  padding-left: 10rpx;
}
.recommend .content .item .bottom {
  font-size: 20rpx;
  color: #ccc;
  display: flex;
  padding-left: 10rpx;
}
.recommend .content .item .bottom .left,
.recommend .content .item .bottom .right {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}
.recommend .content .item .bottom .left text,
.recommend .content .item .bottom .right text {
  margin-right: 10rpx;
}
.recommend .content .item .bottom .left {
  margin-right: 64rpx;
}

