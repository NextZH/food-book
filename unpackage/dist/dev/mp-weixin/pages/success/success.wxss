.background {
  height: 40vh;
  background-image: url(data:image/png;base64,);
  position: relative;
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.background image {
  width: 100rpx;
  height: 100rpx;
  margin-top: 50rpx;
  flex-shrink: 0;
}
.background .title {
  margin: 20rpx;
  color: white;
  font-size: 36rpx;
}
.background .text {
  font-size: 26rpx;
  color: rgba(255, 255, 255, 0.6);
}
.smallBG {
  position: absolute;
  bottom: -160rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 340rpx;
  height: 340rpx;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.smallBG image {
  width: 260rpx;
  height: 260rpx;
}
.topic {
  margin-top: 200rpx;
  text-align: center;
  font-size: 34rpx;
}
.btn {
  margin: 30rpx auto;
  text-align: center;
  line-height: 80rpx;
  width: 70%;
  height: 80rpx;
  border: 4rpx solid #ffae25;
  border-radius: 40rpx;
  font-size: 30rpx;
}
.btn1 {
  background-color: #ffae25;
  color: white;
}
.btn2 {
  background-color: white;
  color: #ffae25;
}

